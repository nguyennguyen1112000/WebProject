<div class="span12">
    <div class="content">
        <div class="module message">
            <div class="module-head">
                <h3>
                    Kết quả học tập</h3>
            </div>
            <div class="module-option clearfix">
                <div class="pull-left">
                    <div class="btn-group">
                        <button class="btn">
                            Tất cả</button>
                        <button class="btn dropdown-toggle" data-toggle="dropdown">
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="/education">Tất cả</a></li>
                            {{#each filters}}

                            <li><a href="/education/?filter={{filter}}">{{filter}}</a></li>
                            {{/each}}

                        </ul>
                    </div>
                </div>
                <div class="pull-right">
                    <a href="#" class="btn btn-primary">In bảng điểm</a>
                </div>
            </div>
            <div class="module-body table">
                <table class="table table-message" id="myTable">
                    <tbody>
                        <tr class="heading">
                            <td class="cell-author hidden-phone hidden-tablet">
                                NH/HK
                            </td>
                            <td>Mã học phần</td>
                            <td class="cell-title">
                                Học phần
                            </td>
                            <td>
                                Số tín chỉ
                            </td>

                            <td class="cell-time align-right">
                                Điểm
                            </td>
                            <td>Ghi chú</td>
                        </tr>
                        {{#each list}}

                        <tr class="unread">
                            <td class="cell-author hidden-phone hidden-tablet">
                                {{NamHoc HocPhan.NgayBatDau HocPhan.NgayKetThuc HocPhan.HocKi}}/{{HocPhan.HocKi}}
                            </td>
                            <td>{{HocPhan.MonHoc.MaMonHoc}}</td>
                            <td class="cell-title">
                                {{HocPhan.MonHoc.MonHoc}}

                            </td>
                            <td>
                                {{HocPhan.MonHoc.SoTC}}

                            </td>

                            <td class="cell-time align-right">
                                {{DiemTK}}
                            </td>
                            <td>{{XepLoai DiemTK}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
            <div class="module-foot">
            </div>
        </div>
    </div>
    <!--/.content-->
</div>