{{#section 'footer'}}
<script>


    $(document).ready(function () {
        $("form").submit(function (e) {
            e.preventDefault()
            var url = $(this).attr("action");
            var request_method = $(this).attr("method")
            frmData = $(this).serialize()
            $.ajax({
                type: request_method,
                url: url,
                data: frmData,
                success: function (data) {
                    if (data.success) {
                        Swal.fire(
                            'Xóa học phần',
                            data.success,
                            'success'
                        )
                            .then(function () {
                                location.reload(true)

                            })
                    }

                }
            });

        })

    });



</script>


{{/section}}
<div class="span9">
    <div class="content">
        <div class="module">
            <div class="module-body">
                <ul class="profile-tab nav nav-tabs">
                    <li class="active"><a href="#student" data-toggle="tab">Sinh viên</a></li>
                    <li><a href="#admin" data-toggle="tab">Quản trị viên</a></li>
                </ul>
                <div class="profile-tab-content tab-content">
                    <div class="tab-pane fade active in" id="student">
                        <div class="module-head">
                            <h3>
                                Danh sách tài khoản</h3>
                        </div>

                        <div class="module-body table scrollme">
                            <table cellpadding="0" cellspacing="0" border="0"
                                class="datatable-1 table table-bordered table-striped display" id="myTable"
                                class="display">
                                <thead>
                                    <tr>
                                        <th>STT</th>
                                        <th>MSSV</th>
                                        <th>Họ và tên</th>
                                        <th>Ngày sinh</th>
                                        <th>Email</th>
                                        <th>Reset mật khẩu</th>
                                        <th>Khóa tài khoản</th>
                                        <th>Xóa tài khoản</th>
                                    </tr>



                                </thead>
                                <tbody>

                                    {{#each list}}
                                    <tr class="odd gradeX" id="row{{@index}}">
                                        <td>{{Add @index 1}}</td>
                                        <td>{{SinhVien.MSSV}}</td>
                                        <td>{{SinhVien.HoLot}} {{SinhVien.Ten}}</td>
                                        <td>{{DateFormat SinhVien.NgaySinh}}</td>
                                        <td>{{SinhVien.Email}}</td>
                                        <td><button class="btn btn-default "><i class="icon-spinner"></i></button> </td>
                                        <td><button class="btn btn-default "><i class="icon-key"></i></button></td>
                                        <td><button class="btn btn-default "><i class="icon-trash"></i></button></td>
                                    </tr>

                                    {{/each}}


                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="admin">
                        dgfgg
                    </div>
                </div>
            </div>
            <!--/.module-body-->
        </div>
        <!--/.module-->
    </div>
    <!--/.content-->
</div>