            <div class="span9">
                <div class="content">

                     <div class="btn-controls">
                        <div class="btn-box-row row-fluid">
                            <a href="/students/add" class="btn-box span3 bg-light">
                                <i class="icon-arrow-up"></i>
                                <b>Upload danh sách</b>
                            </a>
                            <a href="/students/add-new" class="btn-box span3">
                                <i class="icon-user"></i>
                                <b>Thêm sinh viên</b>
                            </a>
                            <a href="/students/find" class="btn-box span3">
                                <i class="icon-search"></i>
                                <b>Tìm kiếm sinh viên</b>
                            </a>
                            <a href="/students/edit-info"  class="btn-box span3">
