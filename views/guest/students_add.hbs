                                <b>Chỉnh sửa sinh viên</b>
                            </a>
                        </div>
                    </div>
                    <!--/.btn-controls-->


                    <div class="module">
                        <div class="module-head">
                            <h3>Upload danh sách sinh viên</h3>
                        </div>
                        <div class="module-body">
                            <form action="" method="post" enctype="multipart/form-data">
                                Chọn file danh sách sinh viên (*)
                                <input type="file" name="file_SV"> <br>
                                <input type="submit" value="upload" class="btn btn-info">
                            </form>
                            <h5>{{msg}}</h5>

                        </div>
                        <div class="module-head">
                            <h3>Danh sách sinh viên</h3>
                        </div>

                        <div class="module-body table">
                            <table cellpadding="0" cellspacing="0" border="0"
                                class="datatable-1 table table-bordered table-striped	 display" width="100%">
                                <thead>
                                    <tr>
                                        <th>STT</th>
                                        <th>MSSV</th>
                                        <th>HoLot</th>
                                        <th>Ten</th>
                                        <th>GioiTinh</th>
                                        <th>NgaySinh</th>
                                        <th>NoiSinh</th>
                                        <th>Lop</th>
                                        <th>Nganh</th>
                                        <th>SoTC</th>
                                        <th>DiemTB</th>
                                        <th>XepLoai</th>
                                    </tr>
                                </thead>
                                <tbody>
                                     {{#each list}}
                                    <tr class="odd gradeX">
                                        <td>{{@index}}</td>
                                        <td>{{MSSV}}</td>
                                        <td>{{HoLot}}</td>
                                        <td>{{Ten}}</td>
                                        <td class="center"> {{GioiTinh}}</td>
                                        <td class="center">{{NgaySinh}}</td>
                                        <td>{{NoiSinh}}</td>
                                        <td>{{Lop}}</td>
                                        <td>{{Nganh}}</td>
                                        <td class="center"> {{SoTC}}</td>
                                        <td class="center">{{DiemTBTichLuy}}</td>
                                        <td class="center">{{XepLoai}}</td>
                                    </tr>
                                     {{/each}}

                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>STT</th>
                                        <th>MSSV</th>
                                        <th>HoLot</th>
                                        <th>Ten</th>
                                        <th>GioiTinh</th>
                                        <th>NgaySinh</th>
                                        <th>NoiSinh</th>
                                        <th>Lop</th>
                                        <th>Nganh</th>
                                        <th>SoTC</th>
                                        <th>DiemTBTichLuy</th>
                                        <th>XepLoai</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>




